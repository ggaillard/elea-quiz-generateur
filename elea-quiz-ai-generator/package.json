{
  "name": "elea-quiz-ai-generator",
  "version": "1.0.0",
  "description": "Générateur de QCM intelligent pour Éléa/Moodle avec analyse automatique de PDF par IA",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "ai:generate": "node scripts/ai-generator.js",
    "pdf:analyze": "node scripts/pdf-analyzer.js",
    "elea:deploy": "node scripts/elea-deploy.js",
    "batch:process": "node scripts/batch-process.js",
    "server": "node server/index.js",
    "mistral:generate": "node scripts/mistral-quiz-generator.cjs generate",
    "mistral:models": "node scripts/mistral-quiz-generator.cjs models",
    "mistral:init": "node scripts/mistral-quiz-generator.cjs init",
    "mistral:install": "./install-mistral.sh",
    "docs:screenshots": "./take-screenshots.sh",
    "docs:placeholders": "./create-placeholders.sh",
    "docs:serve": "npm run dev"
  },
  "dependencies": {
    "@azure/identity": "^4.0.1",
    "@azure/keyvault-secrets": "^4.8.0",
    "@azure/openai": "^1.0.0-beta.11",
    "@azure/storage-blob": "^12.17.0",
    "@langchain/core": "^0.1.48",
    "@langchain/openai": "^0.0.23",
    "@mistralai/mistralai": "^0.1.3",
    "axios": "^1.6.7",
    "cheerio": "^1.0.0-rc.12",
    "clsx": "^2.1.0",
    "commander": "^14.0.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "langchain": "^0.1.29",
    "lucide-react": "^0.323.0",
    "mammoth": "^1.6.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.28.4",
    "pdf-parse": "^1.1.1",
    "pdf2pic": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
